{
  "name": "seeder",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/seed.js",
    "dev": "tsup --watch --onSuccess \"node dist/seed.js\"",
    "build": "tsup",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "jest --detectOpenHandles",
    "seed": "pnpm build && pnpm start"
  },
  "jest": {
    "preset": "@repo/jest-presets/node"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "mongoose": "^8.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@repo/jest-presets": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.11",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  }
}
